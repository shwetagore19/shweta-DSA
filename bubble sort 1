#include<iostream>
using namespace std;
int temp,i,j;

void BubbleSort(int arr[],int n){
for(i=0;i<n-1;i++){
for(j=0;j<n-1-i;j++){
if(arr[j]>arr[j+1]){

temp = arr[j];
arr[j]=arr[j+1];
arr[j+1]=temp;
}
}
}
}

void printArray(int arr[],int n){
for(i=0;i<n;i++){
cout<<arr[i]<<" ";
}
cout<<endl;
}

int main(){
int n=5;
int arr[]={5,7,8,9,2};
BubbleSort(arr,n);
printArray(arr,n);
return 0;




}
